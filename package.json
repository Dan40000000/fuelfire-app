{
  "name": "fuelfire-app",
  "version": "2.1.0",
  "description": "FuelFire - AI-Powered Fitness & Nutrition App with Claude AI meal planning",
  "main": "index.html",
  "scripts": {
    "dev": "npx serve . -p 3000",
    "build": "echo 'Static app with serverless API - ready for deployment'",
    "start": "npx serve . -p 3000",
    "deploy": "vercel --prod",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy",
    "cap:update": "npx cap update",
    "android:open": "npx cap open android",
    "android:run": "npx cap run android",
    "android:build": "cd android && ./gradlew assembleRelease",
    "prepare:android": "npm run cap:sync && npm run cap:copy"
  },
  "keywords": [
    "fitness",
    "nutrition",
    "ai",
    "meal-planning",
    "claude-ai",
    "diet",
    "workout",
    "health",
    "personalized-plans"
  ],
  "author": "Dan Perry",
  "license": "MIT",
  "devDependencies": {
    "serve": "^14.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dan40000000/fuelfire-app.git"
  },
  "bugs": {
    "url": "https://github.com/Dan40000000/fuelfire-app/issues"
  },
  "homepage": "https://github.com/Dan40000000/fuelfire-app#readme",
  "dependencies": {
    "@capacitor-community/speech-recognition": "^7.0.1",
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3"
  }
}
